{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "GroupId": "default",
    "EnableAutoCommit": false,
    "FetchMinBytes": 1,
    "AutoOffsetReset": "Earliest"
  }
}
